{
  "name": "gnoman",
  "version": "0.1.0",
  "description": "GNOMAN 2.0 desktop application for managing Gnosis Safe and wallets",
  "main": "dist/main/main/main.js",
  "bin": {
    "gnoman": "dist/cli/gnoman.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:main": "tsc -p tsconfig.main.json",
    "build:backend": "tsc -p tsconfig.backend.json",
    "build:cli": "tsc -p tsconfig.cli.json",
    "build:renderer": "cd renderer && npm run build",
    "copy:renderer": "node scripts/copyRenderer.js",
    "copy:backend": "node scripts/copyBackendAssets.js",
    "build": "npm run clean && npm run build:main && npm run build:backend && npm run build:cli && npm run copy:backend && npm run build:renderer && npm run copy:renderer",
    "dev:backend": "ts-node-dev --respawn --project tsconfig.backend.json backend/index.ts",
    "dev:renderer": "cd renderer && npm run dev",
    "dev:electron": "npm run build && node scripts/launchElectron.js",
    "dev": "concurrently -k \"npm:dev:backend\" \"npm:dev:renderer\"",
    "start": "npm run build && node scripts/launchElectron.js",
    "start:backend": "node dist/backend/backend/index.js",
    "test": "jest",
    "test:backend": "jest --runTestsByPath tests/keyringManager.test.ts",
    "lint": "eslint .",
    "postinstall": "npm install --prefix renderer"
  },
  "keywords": [
    "electron",
    "gnosis-safe",
    "wallet",
    "ethers"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "axios": "^1.6.8",
    "better-sqlite3": "^9.5.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "electron": "^28.2.0",
    "ethers": "^6.11.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "framer-motion": "^12.23.24",
    "keyring": "^1.1.0",
    "keytar": "^7.9.0",
    "minimist": "^1.2.8",
    "sqlite": "^4.2.1",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "globals": "^13.24.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
